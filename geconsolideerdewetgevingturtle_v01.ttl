line:25573 rdf:value "Art."^^xsd:string .

line:25574 rdf:value "1762bis"^^xsd:string .

line:25575 rdf:value ". <Ingevoegd bij W 30-05-1931, art. 1> De uitdrukkelijke ontbindende voorwaarde wordt voor niet"^^xsd:string .

