line:24269 rdf:value "["^^xsd:string .

line:2427 rdf:value "("^^xsd:string .

line:24270 rdf:value "1"^^xsd:string .

line:24271 rdf:value "- Remedies in geval van leveringsverzuim en van conformiteitsgebrek.]"^^xsd:string .

line:24272 rdf:value "1"^^xsd:string .

