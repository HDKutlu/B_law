line:28102 rdf:value "<Opgeheven bij W 2020-02-04/16, art. 29,10°, 019; Inwerkingtreding : 01-09-2021>"^^xsd:string .

line:28103 rdf:value "TITEL XXI."^^xsd:string .

line:28104 rdf:value "- (KENNISGEVING). <W 2001-10-20/40, art. 3;"^^xsd:string .

