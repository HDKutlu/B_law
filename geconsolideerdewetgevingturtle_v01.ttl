line:26961 rdf:value "Door de (...) dood, ["^^xsd:string .

line:26962 rdf:value "1"^^xsd:string .

line:26963 rdf:value "...]"^^xsd:string .

line:26964 rdf:value "1"^^xsd:string .

line:26965 rdf:value "of het kennelijk onvermogen, hetzij van de lastgever, hetzij van de lasthebber. <W"^^xsd:string .

line:26966 rdf:value "15-12-1949, art. 28>"^^xsd:string .

line:26967 rdf:value "["^^xsd:string .

line:26968 rdf:value "1"^^xsd:string .

line:26969 rdf:value "["^^xsd:string .

line:2697 rdf:value "1"^^xsd:string .

