line:26986 rdf:value "3"^^xsd:string .

line:26987 rdf:value "["^^xsd:string .

line:26988 rdf:value "3"^^xsd:string .

line:26989 rdf:value "De Koning kan de lijst met uitzonderingen, bedoeld in het tweede lid, uitbreiden."^^xsd:string .

line:2699 rdf:value "("^^xsd:string .

line:26990 rdf:value "De lastgeving eindigt in alle gevallen indien de lasthebber komt te verkeren in een staat bedoeld in de artikelen"^^xsd:string .

line:26991 rdf:value "488/1 of 488/2.]"^^xsd:string .

